{
    "name": "@nothing-but/force-graph",
    "version": "0.4.0",
    "license": "MIT",
    "author": "Damian Tarnawski <gthetarnav@gmail.com>",
    "contributors": [],
    "homepage": "https://github.com/thetarnav/nothing-but/tree/main/packages/force-graph#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/thetarnav/nothing-but.git"
    },
    "bugs": {
        "url": "https://github.com/thetarnav/nothing-but/issues"
    },
    "scripts": {
        "dev": "vite serve dev",
        "build:demo": "vite build dev",
        "build": "pnpm tsc -p ./tsconfig.build.json",
        "prepublish": "pnpm build",
        "test": "vitest -c ../../configs/vitest.config.js",
        "typecheck": "echo \"Built with TSC\""
    },
    "private": false,
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "type": "module",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "browser": {},
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        },
        "./canvas": {
            "import": {
                "types": "./dist/canvas.d.ts",
                "default": "./dist/canvas.js"
            }
        },
        "./graph": {
            "import": {
                "types": "./dist/graph.d.ts",
                "default": "./dist/graph.js"
            }
        },
        "./animation": {
            "import": {
                "types": "./dist/animation.d.ts",
                "default": "./dist/animation.js"
            }
        }
    },
    "typesVersions": {
        "*": {
            "canvas": [
                "./dist/canvas.d.ts"
            ],
            "animation": [
                "./dist/animation.d.ts"
            ],
            "graph": [
                "./dist/graph.d.ts"
            ]
        }
    },
    "dependencies": {
        "@nothing-but/dom": "workspace:^",
        "@nothing-but/utils": "workspace:^"
    },
    "devDependencies": {
        "@nothing-but/solid": "workspace:^",
        "@solid-primitives/event-listener": "^2.2.14",
        "@solid-primitives/keyed": "^1.2.0",
        "@solid-primitives/refs": "^1.0.5",
        "@solid-primitives/rootless": "^1.4.2",
        "@solid-primitives/state-machine": "^0.0.1",
        "clsx": "^2.0.0"
    }
}
